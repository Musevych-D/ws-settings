<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorDecreaseFontSize">
    <keyboard-shortcut first-keystroke="alt subtract" />
  </action>
  <action id="EditorIncreaseFontSize">
    <keyboard-shortcut first-keystroke="alt add" />
  </action>
  <action id="EditorSelectWord" />
  <action id="QuickChangeScheme" />
  <action id="Synchronize">
    <keyboard-shortcut first-keystroke="ctrl alt y" />
    <keyboard-shortcut first-keystroke="shift ctrl alt s" />
  </action>
</keymap>